{
	"type":"object",
	"properties":{
		"dpid":{
			"type":"integer",
			"title":"dpid",
			"description":"Datapath ID",
			"readOnly":true,
			"required":true
		},
		"cookie":{
			"type":"integer",
			"title":"cookie",
			"description":"Opaque controller-issued identifier (int)"
		},
		"cookie_mask":{
			"type":"integer",
			"title":"cookie_mask",
			"description":"Mask used to restrict the cookie bits (int)"
		},
		"table_id":{
			"type":"integer",
			"title":"table_id",
			"description":"Table ID to put the flow in (int)"
		},
		"idle_timeout":{
			"type":"integer",
			"title":"idle_timeout",
			"description":"Idle time before discarding (seconds) (int)"
		},
		"hard_timeout":{
			"type":"integer",
			"title":"hard_timeout",
			"description":"Max time before discarding (seconds) (int)"
		},
		"priority":{
			"type":"integer",
			"title":"priority",
			"description":"Priority level of flow entry (int)"
		},
		"buffer_id":{
			"type":"integer",
			"title":"buffer_id",
			"description":"Buffered packet to apply to, or OFP_NO_BUFFER (int)"
		},
		"flags":{
			"type":"integer",
			"title":"flags",
			"description":"Bitmap of OFPFF_* flags (int)"
		},
		"match":{
			"type":"object",
			"title":"match",
			"description":"Fields to match (dict)",
			"properties":{
				"in_port":{
					"type":"integer",
					"title":"in_port",
					"description":"Switch input port (Integer 32bit)"
				},
				"in_phy_port":{
					"type":"integer",
					"title":"in_phy_port",
					"description":"Switch physical input port (Integer 32bit)"
				},
				"metadata":{
					"type":"integer",
					"title":"metadata",
					"description":"Metadata passed between tables (Integer 64bit)"
				},
				"eth_dst":{
					"type":"string",
					"title":"eth_dst",
					"description":"Ethernet destination address (MAC address)"
				},
				"eth_src":{
					"type":"string",
					"title":"eth_src",
					"description":"Ethernet source address (MAC address)"
				},
				"eth_type":{
					"type":"integer",
					"title":"eth_type",
					"description":"Ethernet frame type (Integer 16bit)"
				},
				"vlan_vid":{
					"type":"integer",
					"title":"vlan_vid",
					"description":"VLAN id (Integer 16bit)"
				},
				"vlan_pcp":{
					"type":"integer",
					"title":"vlan_pcp",
					"description":"VLAN priority (Integer 8bit)"
				},
				"ip_dscp":{
					"type":"integer",
					"title":"ip_dscp",
					"description":"IP DSCP (6 bits in ToS field) (Integer 8bit)"
				},
				"ip_ecn":{
					"type":"integer",
					"title":"ip_ecn",
					"description":"IP ECN (2 bits in ToS field) (Integer 8bit)"
				},
				"ip_proto":{
					"type":"integer",
					"title":"ip_proto",
					"description":"IP protocol (Integer 8bit)"
				},
				"ipv4_src":{
					"type":"string",
					"title":"ipv4_src",
					"description":"IPv4 source address (IPv4 address)"
				},
				"ipv4_dst":{
					"type":"string",
					"title":"ipv4_dst",
					"description":"IPv4 destination address (IPv4 address)"
				},
				"tcp_src":{
					"type":"integer",
					"title":"tcp_src",
					"description":"TCP source port (Integer 16bit)"
				},
				"tcp_dst":{
					"type":"integer",
					"title":"tcp_dst",
					"description":"TCP destination port (Integer 16bit)"
				},
				"udp_src":{
					"type":"integer",
					"title":"udp_src",
					"description":"UDP source port (Integer 16bit)"
				},
				"udp_dst":{
					"type":"integer",
					"title":"udp_dst",
					"description":"UDP destination port (Integer 16bit)"
				},
				"sctp_src":{
					"type":"integer",
					"title":"sctp_src",
					"description":"SCTP source port (Integer 16bit)"
				},
				"sctp_dst":{
					"type":"integer",
					"title":"sctp_dst",
					"description":"SCTP destination port (Integer 16bit)"
				},
				"icmpv4_type":{
					"type":"integer",
					"title":"icmpv4_type",
					"description":"ICMP type (Integer 8bit)"
				},
				"icmpv4_code":{
					"type":"integer",
					"title":"icmpv4_code",
					"description":"ICMP code (Integer 8bit)"
				},
				"arp_op":{
					"type":"integer",
					"title":"arp_op",
					"description":"ARP opcode (Integer 16bit)"
				},
				"arp_spa":{
					"type":"string",
					"title":"arp_spa",
					"description":"ARP source IPv4 address (IPv4 address)"
				},
				"arp_tpa":{
					"type":"string",
					"title":"arp_tpa",
					"description":"ARP target IPv4 address (IPv4 address)"
				},
				"arp_sha":{
					"type":"string",
					"title":"arp_sha",
					"description":"ARP source hardware address (MAC address)"
				},
				"arp_tha":{
					"type":"string",
					"title":"arp_tha",
					"description":"ARP target hardware address (MAC address)"
				},
				"ipv6_src":{
					"type":"integer",
					"title":"ipv6_src",
					"description":"IPv6 source address (IPv6 address)"
				},
				"ipv6_dst":{
					"type":"string",
					"title":"ipv6_dst",
					"description":"IPv6 destination address (IPv6 address)"
				},
				"ipv6_flabel":{
					"type":"integer",
					"title":"ipv6_flabel",
					"description":"IPv6 Flow Label (Integer 32bit)"
				},
				"icmpv6_type":{
					"type":"integer",
					"title":"icmpv6_type",
					"description":"ICMPv6 type (Integer 8bit)"
				},
				"icmpv6_code":{
					"type":"integer",
					"title":"icmpv6_code",
					"description":"ICMPv6 code (Integer 8bit)"
				},
				"ipv6_nd_target":{
					"type":"string",
					"title":"ipv6_nd_target",
					"description":"Target address for ND (IPv6 address)"
				},
				"ipv6_nd_sll":{
					"type":"string",
					"title":"ipv6_nd_sll",
					"description":"Source link-layer for ND (MAC address)"
				},
				"ipv6_nd_tll":{
					"type":"string",
					"title":"ipv6_nd_tll",
					"description":"Target link-layer for ND (MAC address)"
				},
				"mpls_label":{
					"type":"integer",
					"title":"mpls_label",
					"description":"MPLS label (Integer 32bit)"
				},
				"mpls_tc":{
					"type":"integer",
					"title":"mpls_tc",
					"description":"MPLS TC (Integer 8bit)"
				},
				"mpls_bos":{
					"type":"integer",
					"title":"mpls_bos",
					"description":"MPLS BoS bit (Integer 8bit)"
				},
				"pbb_isid":{
					"type":"integer",
					"title":"pbb_isid",
					"description":"PBB I-SID (Integer 24bit)"
				},
				"tunnel_id":{
					"type":"integer",
					"title":"tunnel_id",
					"description":"Logical Port Metadata (Integer 64bit)"
				},
				"ipv6_exthdr":{
					"type":"integer",
					"title":"ipv6_exthdr",
					"description":"IPv6 Extension Header pseudo-field (Integer 16bit)"
				},
				"pbb_uca":{
					"type":"integer",
					"title":"pbb_uca",
					"description":"PBB UCA header field (EXT-256 Old version of ONF Extension) (Integer 8bit)"
				},
				"tcp_flags":{
					"type":"integer",
					"title":"tcp_flags",
					"description":"TCP flags (EXT-109 ONF Extension) (Integer 16bit)"
				},
				"actset_output":{
					"type":"integer",
					"title":"actset_output",
					"description":"Output port from action set metadata (EXT-233 ONF Extension) (Integer 32bit)"
				}
			}
		},
		"actions":{
			"type":"array",
			"title":"actions",
			"description":"list of OpenFlow action class (dict)",
			"maxItems":1,
			"items":{
				"type":"object",
				"description":"list of OpenFlow action class (dict)",
				"properties":{
					"action_type":{
						"type":"string",
						"title":"Action type",
						"description":"Action type",
						"required":true,
						"enum":[
							"OUTPUT",
							"COPY_TTL_OUT",
							"COPY_TTL_IN",
							"SET_MPLS_TTL",
							"DEC_MPLS_TTL",
							"PUSH_VLAN",
							"POP_VLAN",
							"PUSH_MPLS",
							"POP_MPLS",
							"SET_QUEUE",
							"GROUP",
							"SET_NW_TTL",
							"DEC_NW_TTL",
							"SET_FIELD",
							"PUSH_PBB",
							"POP_PBB",
							"COPY_FIELD",
							"METER",
							"EXPERIMENTER",
							"GOTO_TABLE",
							"WRITE_METADATA",
							"METER",
							"WRITE_ACTIONS",
							"CLEAR_ACTIONS"
						]
					},
					"action_param":{
						"dependsOn":["action_type"]
					},
					"action_param2":{
						"dependsOn":["action_type"]
					},
					"action_param3":{
						"dependsOn":["action_type"]
					}
				}
			}
		}
	}
}