{
	"type":"object",
	"properties":{
		"dpid":{
			"type":"string",
			"title":"dpid",
			"description":"Datapath ID",
			"readOnly":true,
			"required":true
		},
		"cookie":{
			"type":"string",
			"title":"cookie",
			"description":"Opaque controller-issued identifier (int)"
		},
		"cookie_mask":{
			"type":"string",
			"title":"cookie_mask",
			"description":"Mask used to restrict the cookie bits (int)"
		},
		"table_id":{
			"type":"string",
			"title":"table_id",
			"description":"Table ID to put the flow in (int)"
		},
		"idle_timeout":{
			"type":"string",
			"title":"idle_timeout",
			"description":"Idle time before discarding (seconds) (int)"
		},
		"hard_timeout":{
			"type":"string",
			"title":"hard_timeout",
			"description":"Max time before discarding (seconds) (int)"
		},
		"priority":{
			"type":"string",
			"title":"priority",
			"description":"Priority level of flow entry (int)"
		},
		"buffer_id":{
			"type":"string",
			"title":"buffer_id",
			"description":"Buffered packet to apply to, or OFP_NO_BUFFER (int)"
		},
		"flags":{
			"type":"string",
			"title":"flags",
			"description":"Bitmap of OFPFF_* flags (int)"
		},
		"match":{
			"type":"object",
			"title":"match",
			"description":"in_port | in_phy_port",
			"additionalProperties":{
				"title":"Match parameter",
				"description":"Match parameter vallue",
				"type":"string"
			}
		},
		"actions":{
			"type":"array",
			"title":"actions",
			"description":"list of OpenFlow action class (dict)",
			"maxItems":1,
			"items":{
				"type":"object",
				"description":"list of OpenFlow action class (dict)",
				"properties":{
					"type":{
						"type":"string",
						"title":"Type",
						"description":"Type",
						"required":true,
						"enum":["OUTPUT","COPY_TTL_OUT","COPY_TTL_IN","SET_MPLS_TTL","DEC_MPLS_TTL","POP_VLAN","PUSH_MPLS",
							"POP_MPLS","SET_QUEUE","GROUP","SET_NW_TTL","DEC_NW_TTL","SET_FIELD","PUSH_PBB","POP_PBB",
							"COPY_FIELD","METER","EXPERIMENTER","GOTO_TABLE","WRITE_METADATA","METER","WRITE_ACTIONS",
							"CLEAR_ACTIONS"
						]
					}
				},
				"additionalProperties":{
					"title":"Action parameter",
					"description":"Action parameter vallue",
					"type":"string"
				}
			}
		}
	}
}